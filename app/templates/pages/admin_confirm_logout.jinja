{#=====================================================
Things list page
- List of things passed in as 'things' array
- Form for adding a new thing
=====================================================#}

{% extends 'pages/base.jinja' %}


{% block title %}

Flask Demo - All Things

{% endblock %}

{% block content %}
<div id="confirm_title">
    <h2>Are you sure you want to logout?</h2>
</div>
<div id="confirm_form">
    <form method="POST" action="/logout">
        <button id="button_width" type="submit">Yes, logout</button>
        <a role="button" href="/admin_home">Cancel</a>
    </form>
</div>


{% endblock %}